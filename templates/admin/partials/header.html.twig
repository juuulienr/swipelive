<!-- header -->
<header class="css-2bi464">
  <div class="header--left">
    <div class="wrapper--icon">
      <div>
        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true" role="img" width="1em" height="1em" preserveAspectRatio="xMidYMid meet" viewBox="0 0 24 24"><g fill="currentColor"><circle cx="4" cy="12" r="1"></circle><rect x="7" y="11" width="14" height="2" rx=".94" ry=".94"></rect><rect x="3" y="16" width="18" height="2" rx=".94" ry=".94"></rect><rect x="3" y="6" width="18" height="2" rx=".94" ry=".94"></rect></g></svg>
      </div>
    </div>
    <button class="header--search">
      <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true" role="img" class="MuiBox-root css-1t9pz9x iconify iconify--eva" sx="[object Object]" width="1em" height="1em" preserveAspectRatio="xMidYMid meet" viewBox="0 0 24 24"><path fill="currentColor" d="M20.71 19.29l-3.4-3.39A7.92 7.92 0 0 0 19 11a8 8 0 1 0-8 8a7.92 7.92 0 0 0 4.9-1.69l3.39 3.4a1 1 0 0 0 1.42 0a1 1 0 0 0 0-1.42zM5 11a6 6 0 1 1 6 6a6 6 0 0 1-6-6z"></path></svg>
    </button>
    <form class="header--search-bar">
      <div class="search-bar--input">
        <label for="search">
          <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true" role="img" class="MuiBox-root css-1q8h0dm iconify iconify--eva" sx="[object Object]" width="1em" height="1em" preserveAspectRatio="xMidYMid meet" viewBox="0 0 24 24"><path fill="currentColor" d="M20.71 19.29l-3.4-3.39A7.92 7.92 0 0 0 19 11a8 8 0 1 0-8 8a7.92 7.92 0 0 0 4.9-1.69l3.39 3.4a1 1 0 0 0 1.42 0a1 1 0 0 0 0-1.42zM5 11a6 6 0 1 1 6 6a6 6 0 0 1-6-6z"></path></svg>
        </label>
        <input type="text" id="search" placeholder="Rechercher..." style="padding-left: 10px;">
      </div>
      <button id="searchBtn">Rechercher</button>
    </form>
  </div>
  <div class="header--right">
    <div class="header--notifications">
      <button class="header--notification--btn"> 
        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true" role="img" class="MuiBox-root css-1t9pz9x iconify iconify--eva" sx="[object Object]" width="1em" height="1em" preserveAspectRatio="xMidYMid meet" viewBox="0 0 24 24"><path fill="currentColor" d="M20.52 15.21l-1.8-1.81V8.94a6.86 6.86 0 0 0-5.82-6.88a6.74 6.74 0 0 0-7.62 6.67v4.67l-1.8 1.81A1.64 1.64 0 0 0 4.64 18H8v.34A3.84 3.84 0 0 0 12 22a3.84 3.84 0 0 0 4-3.66V18h3.36a1.64 1.64 0 0 0 1.16-2.79zM14 18.34A1.88 1.88 0 0 1 12 20a1.88 1.88 0 0 1-2-1.66V18h4z"></path></svg>
        <span class="notif--number">2</span>
      </button>
      <div class="menu--down menu--down--notif">
        <span class="arrow--top--right"></span>
        <div>
          <div class="notif--head">
            <div class="notif--head--left">
              <h6>Notifications</h6>
              <p>You have 2 unread messages</p>
            </div>
            <button>
              <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true" role="img" class="MuiBox-root css-1t9pz9x iconify iconify--eva" sx="[object Object]" width="1em" height="1em" preserveAspectRatio="xMidYMid meet" viewBox="0 0 24 24"><g fill="currentColor"><path d="M16.62 6.21a1 1 0 0 0-1.41.17l-7 9l-3.43-4.18a1 1 0 1 0-1.56 1.25l4.17 5.18a1 1 0 0 0 .78.37a1 1 0 0 0 .83-.38l7.83-10a1 1 0 0 0-.21-1.41zm5 0a1 1 0 0 0-1.41.17l-7 9l-.61-.75l-1.26 1.62l1.1 1.37a1 1 0 0 0 .78.37a1 1 0 0 0 .78-.38l7.83-10a1 1 0 0 0-.21-1.4z"></path><path d="M8.71 13.06L10 11.44l-.2-.24a1 1 0 0 0-1.43-.2a1 1 0 0 0-.15 1.41z"></path></g></svg>
            </button>
          </div>
          <hr>
          <div class="notif--name">NEW</div>
          <div class="notif--bloc--container">
            <div class="notif--bloc notif--bloc--unread">
              <div class="notif--bloc--icon">
                <img src="{{ app.request.baseUrl }}/img/ic_notification_package.svg" alt="Notification icon">
              </div>
              <div class="notif--bloc--content">
                <h6>Your order is placed
                  <span>waiting for shipping</span>
                </h6>
                <div class="notif--bloc--details">
                  <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true" role="img" class="MuiBox-root css-13s3r1o iconify iconify--eva" sx="[object Object]" width="1em" height="1em" preserveAspectRatio="xMidYMid meet" viewBox="0 0 24 24"><g fill="currentColor"><path d="M12 2a10 10 0 1 0 10 10A10 10 0 0 0 12 2zm0 18a8 8 0 1 1 8-8a8 8 0 0 1-8 8z"></path><path d="M16 11h-3V8a1 1 0 0 0-2 0v4a1 1 0 0 0 1 1h4a1 1 0 0 0 0-2z"></path></g></svg>
                  35 minutes ago
                </div>
              </div>
            </div>
            <div class="notif--bloc notif--bloc--unread">
              <div class="notif--bloc--icon">
                <img src="{{ app.request.baseUrl }}/img/avatar_3.jpg" alt="Notification icon">
              </div>
              <div class="notif--bloc--content">
                <h6>Sylvan King
                  <span>answered to your comment on the minimal</span>
                </h6>
                <div class="notif--bloc--details">
                  <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true" role="img" class="MuiBox-root css-13s3r1o iconify iconify--eva" sx="[object Object]" width="1em" height="1em" preserveAspectRatio="xMidYMid meet" viewBox="0 0 24 24"><g fill="currentColor"><path d="M12 2a10 10 0 1 0 10 10A10 10 0 0 0 12 2zm0 18a8 8 0 1 1 8-8a8 8 0 0 1-8 8z"></path><path d="M16 11h-3V8a1 1 0 0 0-2 0v4a1 1 0 0 0 1 1h4a1 1 0 0 0 0-2z"></path></g></svg>
                  1 day ago
                </div>
              </div>
            </div>
          </div>
          <div class="notif--name">Before that</div>
          <div class="notif--bloc--container">
            <div class="notif--bloc">
              <div class="notif--bloc--icon">
                <img src="{{ app.request.baseUrl }}/img/ic_notification_chat.svg" alt="Notification icon">
              </div>
              <div class="notif--bloc--content">
                <h6>You have new message
                  <span>5unread messages</span>
                </h6>
                <div class="notif--bloc--details">
                  <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true" role="img" class="MuiBox-root css-13s3r1o iconify iconify--eva" sx="[object Object]" width="1em" height="1em" preserveAspectRatio="xMidYMid meet" viewBox="0 0 24 24"><g fill="currentColor"><path d="M12 2a10 10 0 1 0 10 10A10 10 0 0 0 12 2zm0 18a8 8 0 1 1 8-8a8 8 0 0 1-8 8z"></path><path d="M16 11h-3V8a1 1 0 0 0-2 0v4a1 1 0 0 0 1 1h4a1 1 0 0 0 0-2z"></path></g></svg>
                  2 days ago
                </div>
              </div>
            </div>
            <div class="notif--bloc">
              <div class="notif--bloc--icon">
                <img src="{{ app.request.baseUrl }}/img/ic_notification_mail.svg" alt="Notification icon">
              </div>
              <div class="notif--bloc--content">
                <h6>You have new mail
                  <span>sent from guido padberg</span>
                </h6>
                <div class="notif--bloc--details">
                  <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true" role="img" class="MuiBox-root css-13s3r1o iconify iconify--eva" sx="[object Object]" width="1em" height="1em" preserveAspectRatio="xMidYMid meet" viewBox="0 0 24 24"><g fill="currentColor"><path d="M12 2a10 10 0 1 0 10 10A10 10 0 0 0 12 2zm0 18a8 8 0 1 1 8-8a8 8 0 0 1-8 8z"></path><path d="M16 11h-3V8a1 1 0 0 0-2 0v4a1 1 0 0 0 1 1h4a1 1 0 0 0 0-2z"></path></g></svg>
                  3 days ago
                </div>
              </div>
            </div>
            <div class="notif--bloc">
              <div class="notif--bloc--icon">
                <img src="{{ app.request.baseUrl }}/img/ic_notification_package.svg" alt="Notification icon">
              </div>
              <div class="notif--bloc--content">
                <h6>Delivery processing
                  <span>your order is being shipped</span>
                </h6>
                <div class="notif--bloc--details">
                  <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true" role="img" class="MuiBox-root css-13s3r1o iconify iconify--eva" sx="[object Object]" width="1em" height="1em" preserveAspectRatio="xMidYMid meet" viewBox="0 0 24 24"><g fill="currentColor"><path d="M12 2a10 10 0 1 0 10 10A10 10 0 0 0 12 2zm0 18a8 8 0 1 1 8-8a8 8 0 0 1-8 8z"></path><path d="M16 11h-3V8a1 1 0 0 0-2 0v4a1 1 0 0 0 1 1h4a1 1 0 0 0 0-2z"></path></g></svg>
                  4 days ago
                </div>
              </div>
            </div>
          </div>
          <hr>
          <div class="menu--notif--btn">
            <button>View all</button>
          </div>
        </div>
      </div>
    </div>
