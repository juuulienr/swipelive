{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^8.2",
    "ext-iconv": "*",
    "aws/aws-sdk-php": "*",
    "boogiefromzk/agora-token": "^1.0",
    "bugsnag/bugsnag-symfony": "^1.13",
    "cloudinary/cloudinary_php": "^2.11",
    "composer/package-versions-deprecated": "~1.10",
    "doctrine/annotations": "^2.0",
    "doctrine/doctrine-bundle": "^2.9",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^2.15",
    "guzzlehttp/guzzle": "^7.4",
    "kreait/firebase-php": "^7.15",
    "lexik/jwt-authentication-bundle": "^3.1",
    "logtail/monolog-logtail": "^2.2",
    "nelmio/cors-bundle": "^2.3",
    "php-ffmpeg/php-ffmpeg": "^1.2",
    "phpdocumentor/reflection-docblock": "^5.2",
    "pusher/pusher-php-server": "^7.0",
    "sensio/framework-extra-bundle": "^6.2",
    "stripe/stripe-php": "^10.11",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "^6.4",
    "symfony/console": "^6.4",
    "symfony/expression-language": "^6.4",
    "symfony/flex": "^2.0",
    "symfony/form": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-client": "^6.4",
    "symfony/mailer": "^6.4",
    "symfony/mime": "^6.4",
    "symfony/monolog-bundle": "^3.8",
    "symfony/process": "^6.4",
    "symfony/property-access": "^6.4",
    "symfony/property-info": "^6.4",
    "symfony/security-bundle": "^6.4",
    "symfony/serializer": "^6.4",
    "symfony/translation": "^6.4",
    "symfony/twig-bundle": "^6.4",
    "symfony/validator": "^6.4",
    "symfony/web-link": "^6.4",
    "symfony/webpack-encore-bundle": "^2.0",
    "symfony/yaml": "^6.4",
    "twig/extra-bundle": "^3.0",
    "twig/string-extra": "^3.6",
    "twig/twig": "^3.0",
    "viktorruskai/facebook-graph-sdk": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "symfony/browser-kit": "^6.4",
    "symfony/css-selector": "^6.4",
    "symfony/debug-bundle": "^6.4",
    "symfony/dotenv": "^6.4",
    "symfony/maker-bundle": "^1.43.0",
    "symfony/phpunit-bridge": "^6.4",
    "symfony/stopwatch": "^6.4",
    "symfony/web-profiler-bundle": "^6.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true,
      "php-http/discovery": false
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "compile": [
      "php bin/console doctrine:schema:update --force --complete"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^6.4"
    }
  }
}
