{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^8.1",
    "ext-iconv": "*",
    "bugsnag/bugsnag-symfony": "^1.13",
    "cloudinary/cloudinary_php": "^2.11",
    "composer/package-versions-deprecated": "~1.10",
    "doctrine/annotations": "~1.0",
    "doctrine/doctrine-bundle": "~2.7",
    "doctrine/doctrine-fixtures-bundle": "~3.4",
    "doctrine/doctrine-migrations-bundle": "~2.2",
    "doctrine/orm": "~2.15",
    "guzzlehttp/guzzle": "^7.4",
    "lexik/jwt-authentication-bundle": "~2.19",
    "nelmio/cors-bundle": "~2.3",
    "phpdocumentor/reflection-docblock": "~5.2",
    "pusher/pusher-php-server": "^7.0",
    "sensio/framework-extra-bundle": "^6.2",
    "sngrl/php-firebase-cloud-messaging": "dev-master",
    "stripe/stripe-php": "^10.11",
    "symfony/apache-pack": "~1.0",
    "symfony/asset": "^5.4",
    "symfony/console": "^5.4",
    "symfony/expression-language": "^5.4",
    "symfony/flex": "~1.20",
    "symfony/form": "^5.4",
    "symfony/framework-bundle": "^5.4",
    "symfony/http-client": "^5.4",
    "symfony/mailer": "^5.4",
    "symfony/mime": "^5.4",
    "symfony/monolog-bundle": "~3.0",
    "symfony/process": "^5.4",
    "symfony/property-access": "^5.4",
    "symfony/property-info": "^5.4",
    "symfony/security-bundle": "^5.4",
    "symfony/serializer": "^5.4",
    "symfony/translation": "^5.4",
    "symfony/twig-bundle": "^5.4",
    "symfony/validator": "^5.4",
    "symfony/web-link": "^5.4",
    "symfony/webpack-encore-bundle": "^2.0",
    "symfony/yaml": "^5.4",
    "twig/extra-bundle": "^3.0",
    "twig/string-extra": "^3.6",
    "twig/twig": "^3.0",
    "viktorruskai/facebook-graph-sdk": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "symfony/browser-kit": "^5.4",
    "symfony/css-selector": "^5.4",
    "symfony/debug-bundle": "^5.4",
    "symfony/dotenv": "^5.4",
    "symfony/maker-bundle": "1.43.0",
    "symfony/phpunit-bridge": "~6.0",
    "symfony/stopwatch": "^5.4",
    "symfony/web-profiler-bundle": "^5.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true,
      "php-http/discovery": false
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "compile": [
      "php bin/console doctrine:schema:update --force --complete"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^5.4"
    }
  }
}
